= bootstrap_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f|
  = devise_error_messages!
  .col-lg-6.col-lg-offset-3
    .panel.panel-default
      .panel-heading
        %h1.panel-title Sign up
      .panel-body 
        %div
          = f.text_field :name, autofocus: true, placeholder: "Enter full name"
        %div
          = f.email_field :email, placeholder: "Enter QueensU email"
        %div
          = f.password_field :password, autocomplete: "off", placeholder: valid
        %div
          = f.password_field :password_confirmation, autocomplete: "off", placeholder: valid
        %div= f.submit "Sign up", class: "btn btn-default btn-md btn-block"
-# = render "devise/shared/links"
